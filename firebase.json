{
    "firebase": "reactfb",
    "public": "dist",
    "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/bower_components/**"
    ],
    "headers": [
        {
            "source": "**/*.@(html|js)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000, public"
                }
            ]
        }
    ]
}
